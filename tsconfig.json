{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "*":["src/*"],
            "src":["src/*"],
            "components":["src/components/*"],
            "screens":["src/screens/*"],
            "configs":["src/configs/*"],
            "middlewares":["src/middlewares/*"],
            "res":["src/res/*"],
            "routes":["src/routes/*"],
            "library":["src/library/*"],
            "utils":["src/utils/*"],
            "images":["src/res/images/*"],
        },
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "jsx": "react",
        "lib": ["es6"],
        "moduleResolution": "node",
        "noEmit": true,
        "strict": true,
        "target": "esnext",
        "resolveJsonModule": true
    },
        "exclude": [
          "node_modules",
          "babel.config.js",
          "metro.config.js",
          "jest.config.js"
        ]
}